# SteemMaze Environment Variables
# Copy this file to .env and fill in your actual values
# NEVER commit your .env file to git!

# Steem Registry Account Configuration
# This is the posting key for the steemmaze registry account
# The posting key allows broadcasting custom_json only (no fund transfers)
STEEM_REGISTRY_POSTING_KEY=YOUR_POSTING_KEY_HERE

# Optional: Custom Steem API node
# STEEM_CUSTOM_NODE=https://api.steemit.com
